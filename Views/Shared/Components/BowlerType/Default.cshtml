@model IEnumerable<Teams>

<a asp-action="Index" asp-controller="Home"
   asp-route-bowlertype="">Home</a>
<br />


@foreach (var x in Model)
{
    <a asp-action="Index" asp-controller="Home"
       asp-route-bowlertype="@x.TeamId"
       asp-route-teamtype="@x.TeamName"
       >@x.TeamName</a>
    <br />
}
